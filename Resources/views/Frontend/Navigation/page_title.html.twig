{% block title -%}
    {%- if element_page_title is defined and element_override_page_title is defined and element_override_page_title -%}
        {{ element_page_title|trans }}
    {%- else -%}
        {%- if element_page_title is defined and element_page_title -%}
            {{ element_page_title }} - {% endif -%}
        {%- for element in elements -%}
            {% set entityTitle = element ~ '' %}
            {% if element['metaTitle'] is defined and element['metaTitle'] and (element['metaTitleOverride'] is not defined or not element['metaTitleOverride']) %}
                {% set entityTitle = element.metaTitle %}
            {% endif %}
            {% set entityTitle = entityTitle|transTitle|replace({'<': ' <'})|striptags|truncate(100)|raw %}
            {%- if (element.id is defined and element.id is null) or element.id is not defined -%}
                {% set entityTitle = entityTitle|trans %}
            {%- endif -%}
            {{ entityTitle }} - {% endfor -%}
        {{ project_title|trans }}
    {%- endif %}
{%- endblock -%}