{% block meta_viewport %}
    <meta name="viewport" content="width=device-width, initial-scale=1">
{% endblock %}

{% block meta_format_detection %}
    <meta name="format-detection" content="telephone=no">
{% endblock %}

{% block meta_description %}
    {{ render_esi(controller('UnifikSystemBundle:Frontend/Metadata:metadata', { metaName: 'description' })) }}
{% endblock %}

{% block meta_keywords %}
    {{ render_esi(controller('UnifikSystemBundle:Frontend/Metadata:metadata', { metaName: 'keywords' })) }}
{% endblock %}

{% block meta_robots %}
    <meta name="robots" content="index,follow" />
{% endblock %}